		<stream name="temperaturEmitter" port="1337">
			<attr name="temp" type="int" />
			<attr name="rstring" type="string" />
			<attr name="timestamp" type="timestamp" />
		</stream>
		<stream name="humidityEmitter" port="1338">
		    <attr name="Humidity" type="int" />
			<attr name="rstring" type="string" />
			<attr name="timestamp" type="timestamp" />
		</stream>
	</stream-processor>
</streamdb>